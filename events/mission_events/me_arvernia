me_arvernia.6 = {
    type = country_event

    title = me_arvernia_6_title
    desc = me_arvernia_6_desc
    picture = great_library

    option = {
        name = "me_arvernia_6.a"
        custom_tooltip = me_arvernia_6_tooltip
        hidden_effect = {
            every_country = {
                limit = {
                    OR = {
                        country_culture_group = indian
                        country_culture_group = aryan
                    }
                    NOT = {
                        this = root
                    }
                }
                add_opinion = {
                    modifier = opinion_invaded_maldives
                    target = root  
                }
            }
        }
    }
}
