												}
											}
											widget = {
												size = { 8 0 }
												visible = "[Not( LawGroupItem.HasClans )]"
											}
										}
									}
									flowcontainer = {
										position = { 0 36 }
										
										dynamicgridbox = {
											datamodel = "[LawGroupItem.GetLaws]"
											
											item = {
												widget = {
													size = { 280 32 }

													
													alpha = "[TransparentIfFalse(LawItem.IsAllowed)]"

													button_radial = {
														position = { 4 0 }
														enabled = "[And( LawItem.CanEnact, LawItem.IsAllowed )]"
														scale = 0.9
														
														blockoverride "On_click" 
														{
															onclick = "[LawItem.Enact]"
